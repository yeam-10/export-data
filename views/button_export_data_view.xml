<?xml version="1.0"?>
<odoo>



	<record id="export_data_form" model="ir.ui.view">
		<field name="name">export.data.form</field>
		<field name="model">export.data</field>
		<field name="arch" type="xml">
			<form string="button">
				<sheet>
                   
					   
                       <button name="action_view_export_data" type="action"  string="Export data"/>
					
                 
				</sheet>
			</form>
		</field>
    </record>



    <record id="action_export_data" model="ir.actions.act_window">
            <field name="name">Button</field>
           <field name="type">ir.actions.act_window</field>
            <field name="res_model">export.data</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="export_data_form"/>
          
    </record>

    <menuitem id="menu_export_data"
            name="Export data"
            groups="base.group_system"
            action="action_export_data"
            sequence="0"/>


   
</odoo>
